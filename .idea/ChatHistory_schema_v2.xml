+            result.toObjects(FirestoreFollowing::class.java)\n+            \n+        } catch (e: Exception) {\n+            Log.e(TAG, \&quot;Error getting followers\&quot;, e)\n+            emptyList()\n+        }\n+    }\n+    \n+    /**\n+     * Check if user A is following user B\n+     */\n+    suspend fun isFollowing(followerUid: String, followingUid: String): Boolean {\n+        return try {\n+            val result \u003d followingCollection\n+                .whereEqualTo(\&quot;followerUid\&quot;, followerUid)\n+                .whereEqualTo(\&quot;followingUid\&quot;, followingUid)\n+                .get()\n+                .await()\n+            \n+            result.documents.isNotEmpty()\n+            \n+        } catch (e: Exception) {\n+            Log.e(TAG, \&quot;Error checking if following\&quot;, e)\n+            false\n+        }\n+    }\n+    \n+    /**\n+     * Unfollow a user\n+     */\n+    suspend fun unfollowUser(followerUid: String, followingUid: String): Boolean {\n+        return try {\n+            val result \u003d followingCollection\n+                .whereEqualTo(\&quot;followerUid\&quot;, followerUid)\n+                .whereEqualTo(\&quot;followingUid\&quot;, followingUid)\n+                .get()\n+                .await()\n+            \n+            result.documents.forEach { doc -\u003e\n+                doc.reference.delete().await()\n+            }\n+            \n+            Log.d(TAG, \&quot;User unfollowed successfully\&quot;)\n+            true\n+            \n+        } catch (e: Exception) {\n+            Log.e(TAG, \&quot;Error unfollowing user\&quot;, e)\n+            false\n+        }\n+    }\n+}\n```\n\nThe edit was successful with no linter errors.&quot;},{&quot;role&quot;:&quot;assistant&quot;,&quot;content&quot;:[{&quot;type&quot;:&quot;text&quot;,&quot;text&quot;:&quot;Now I need to update the SocialViewModel to use the Firestore social repository:&quot;}],&quot;tool_calls&quot;:[{&quot;id&quot;:&quot;tool